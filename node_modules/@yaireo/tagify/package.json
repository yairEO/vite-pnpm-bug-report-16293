        "build": "gulp",
        "version": "gulp build && git add .",
        "prepublishOnly": "pkg-ok",
        "test": "echo \"No test specified\"",
        "serve": "npx http-server -o index.html -c-1"
    },
    "jest": {
        "preset": "jest-puppeteer"
    },
